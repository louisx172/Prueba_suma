#include <iostream>
using namespace std;

int main() {
    // Números predefinidos
    int num1 = 10;
    int num2 = 20;

    // Sumar los números
    int suma = num1 + num2;

    // Mostrar el resultado
    cout << "La suma de " << num1 << " y " << num2 << " es: " << suma << endl;

    return 0;
}
